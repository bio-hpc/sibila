{
    "model": "ANN",
    "type_ml": "classification",
    "n_job": 1,
    "params": {
        "draw_model": true,
        "random_state": 500,
        "cv_splits": 10,
        "batch_size": 1024,
        "search_type": "random"
    },
    "params_grid": {
        "objective": ["accuracy"],
        "max_layers": 3,
        "min_units": 4,
        "max_units": 512,
        "step_units": 1,
        "output_units": 2,
        "activation": ["relu","elu","tanh","sigmoid","softmax","linear","exponential"],
        "dropout": true,
        "dropout_rate": 0.15,
        "optimizer": ["Adam", "RMSprop", "SGD", "Adagrad"],
        "min_lr": 1e-4,
        "max_lr": 1e-2,
        "sampling_lr": "log",
        "metrics": ["accuracy"],
        "loss_function": "binary_crossentropy",
        "epochs": 300,
        "executions_per_trial": 5,
        "early_stopping_patience": 3
    }
}
